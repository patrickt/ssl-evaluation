name:                ssl-evaluation
version:             0.1.0.0
synopsis:            Evaluating the state of enumerator-like HTTP libraries supporting SSL.
-- description:         
homepage:            github.com/patrickt/ssl-evaluation
license:             PublicDomain
license-file:        LICENSE
author:              Patrick Thomson
maintainer:          patrick.william.thomson@gmail.com
-- copyright:           
category:            Network
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

executable ssl-evaluation
  main-is:             Main.hs
  other
  build-depends:       base == 4.6.*
                     , aeson == 0.7.0.*
                     , bytestring == 0.10.0.*
                     , connection == 0.1.3.*
                     , http-conduit == 2.0.0.*
                     , text == 0.11.3.*
                     , tls == 1.1.5.*
                     , tls-extra == 0.6.6.*
  default-language:    Haskell2010
  default-extensions:  OverloadedStrings